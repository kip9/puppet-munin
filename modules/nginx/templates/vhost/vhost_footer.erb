<% if includes -%>
  <% includes.each do |single_include| -%>
  include <%= single_include %>;
  <% end -%>
<% end -%>
}

<% if rewrite_www_to_non_www %>
server {
  listen                <%= listen_ip %>;
  server_name           www.<%= name.gsub(/^www\./, '') %>;
  rewrite               ^ http://<%= name.gsub(/^www\./, '') %>$uri permanent;
}
<% end %>
